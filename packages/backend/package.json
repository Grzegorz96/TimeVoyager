        "express-session": "^1.18.1",
        "mongoose": "^8.7.3",
        "passport": "^0.7.0",
        "passport-discord": "^0.1.4",
        "passport-local": "^1.0.0",
        "passport-oauth2": "^1.8.0",
        "redis": "^4.7.0",
        "zod": "^3.23.8"
